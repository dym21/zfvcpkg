{
  "name": "libemf",
  "version": "1.0.13",
  "description": "Enhanced Metafile (EMF) library for reading and writing EMF files",
  "homepage": "https://sourceforge.net/projects/libemf/",
  "license": "LGPL-2.1",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "tools": {
      "description": "Build EMF tools (emf2svg, emf2eps, etc.)",
      "dependencies": [
        {
          "name": "libpng",
          "default-features": false
        }
      ]
    }
  }
}
